{
  "name": "Synapse",
  "version": "1.0.0",
  "description": "Connecting ideas.",
  "main": "main.js",
  "scripts": {
    "start": "ENVIRONMENT=DEV ./node_modules/.bin/electron .",
    "build": "./node_modules/.bin/webpack",
    "remotedev": "remotedev --hostname=localhost --port=8000",
    "watch": "./node_modules/.bin/webpack-dev-server --hot --inline",
    "osx-package": "./node_modules/.bin/webpack -p && ./node_modules/electron-packager/cli.js ./ Synapse --out ./bin --platform=darwin --arch=x64 --version=0.34.0 --overwrite --ignore=\"ignore|bin|node_modules\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freddieiboy/synapse.git"
  },
  "author": "Freddie Iboy",
  "devDependencies": {
    "babel": "6.5.2",
    "babel-core": "6.9.0",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "css-loader": "0.23.1",
    "electron-packager": "7.0.2",
    "electron-prebuilt": "^1.0.1",
    "node-sass": "3.7.0",
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "remote-redux-devtools": "0.3.3-alpha2",
    "sass-loader": "3.2.0",
    "style-loader": "0.13.1",
    "webpack": "1.13.0",
    "webpack-dev-server": "1.14.0"
  },
  "dependencies": {
    "jquery": "2.2.3",
    "react-redux": "4.4.5",
    "redux": "3.5.2",
    "redux-thunk": "2.1.0"
  }
}
